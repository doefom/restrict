{
    "name": "doefom/restrict",
    "description": "A Statamic addon that lets you define which entries should be listable to whom in the control panel by defining custom restriction logic.",
    "autoload": {
        "psr-4": {
            "Doefom\\Restrict\\": "src"
        }
    },
    "require": {
        "statamic/cms": "^v4.0|^v5.0"
    },
    "extra": {
        "statamic": {
            "name": "Restrict",
            "description": "Restrict addon"
        },
        "laravel": {
            "providers": [
                "Doefom\\Restrict\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "license": "MIT"
}
